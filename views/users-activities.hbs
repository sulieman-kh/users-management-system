<div class="container">
  <table class="table table-fluid" id="myTable">
    <thead class="thead-dark">
      <tr style="text-align: center;">
        <th scope="col">#</th>
        <th scope="col">фото пользователя</th>
        <th scope="col">Активность пользователей</th>
      </tr>
    </thead>
    <tbody>
      {{#each rows}}
        <tr style="text-align: center;">
          <th scope="row">{{this.id}}</th>
          <th>
            {{#if this.userimage}}
              <img
                class="card-image"
                style="width: 75px; height: 75px; border-radius: 50%"
                src="{{this.userimage}}"
                alt="user-profile"
              />
            {{else}}
              <img
                class="card-image"
                src="person-svgrepo-com.svg"
                alt="user-profile"
                style="width: 75px; height: 75px; border-radius: 50%"
              />
            {{/if}}
          </th>
          <td>{{this.activity}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<script>
  $(document).ready( function () { $('#myTable').DataTable(); } );
  $('table').DataTable({searching: false, info:false, "pageLength": 3, "dom":
  'rtip'});
</script>
